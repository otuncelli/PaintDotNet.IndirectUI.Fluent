<?xml version="1.0" encoding="utf-8"?>
<IndirectUI xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Schema\IndirectUI.xsd">
	<Panel Name="First" Text="First Tab">
		<Controls>
			<AngleChooser Name="AngleChooser1" />
			<CheckBox Name="CheckBox1" 
					  DisplayName="Make ReadOnly 1" />
			<ColorWheel Name="ColorWheel1" 
						DisplayName="Test Color Wheel" 
						Value="#FFFF0000" 
						Alpha="true">
				<BoundToBoolean SourceControl="CheckBox1" />
			</ColorWheel>
			<IntSlider Name="IntSlider1"
                       DisplayName="Hello IntSlider"
                       MinValue="50"
                       MaxValue="100"
                       ShowResetButton="false">
				<BoundToBoolean SourceControl="CheckBox1" />
				<ControlColors>
					<Color>#FF0000</Color>
					<Color>#00FF00</Color>
					<Color>#0000FF</Color>
				</ControlColors>
			</IntSlider>
			<Container>
				<Panel Name="Nested First" 
					   Text="Nested First Tab">
					<Controls>
						<CheckBox Name="CheckBox2" 
								  DisplayName="Make ReadOnly 2" />
						<DoubleSlider Name="DoubleSlider1"
                                      DisplayName="Hello DoubleSlider1"
                                      MinValue="0"
                                      MaxValue="1"
                                      SliderSmallChange=".01"
                                      SliderLargeChange=".05"
                                      UpDownIncrement=".01"
                                      DecimalPlaces="2">
							<BoundToBoolean SourceControl="CheckBox2" />
						</DoubleSlider>
						<DoubleVector3RollBallAndSlider Name="RollBallAndSlider1">
							<BoundToBoolean SourceControl="CheckBox2" 
											Inverse="true" />
						</DoubleVector3RollBallAndSlider>
						<DoubleVector3Slider Name="Vector3Slider1">
							<BoundToBoolean SourceControl="CheckBox2" />
						</DoubleVector3Slider>
					</Controls>
				</Panel>
				<Panel Name="Nested Second" 
					   Text="Nested Second Tab">
					<Controls>
						<CheckBox Name="CheckBox3" 
								  DisplayName="Make ReadOnly 3" />
						<DoubleVectorPanAndSlider Name="PanAndSlider"
                                                  StaticImageUnderlay="">
							<BoundToBoolean SourceControl="CheckBox3" />
						</DoubleVectorPanAndSlider>
						<DropDown Name="DropDown1"
                                  DisplayName="Menü"
                                  Description="Description here...">
							<BoundToBoolean SourceControl="CheckBox3" />
							<Choice>Choice 1</Choice>
							<Choice>Choice 2</Choice>
							<Choice>Choice 3</Choice>
						</DropDown>
						<FileChooser Name="FileChooser1"
                                     AllowAllFiles="true">
						</FileChooser>
						<FolderChooser Name="FolderChooser1" />
					</Controls>
				</Panel>
			</Container>
		</Controls>
	</Panel>
	<Panel Name="Second" Text="Second Tab">
		<Controls>
			<CheckBox Name="CheckBox4" 
					  DisplayName="Make ReadOnly 4" />
			<IncrementButton Name="IncrementButton1" 
							 ButtonText="Click Me!">
				<BoundToBoolean SourceControl="CheckBox4" />
			</IncrementButton>
			<LinkLabel Name="LinkLabel1" 
					   DisplayName="Google" 
					   Value="http://www.google.com" />
			<RadioButton Name="RadioButton1" 
						 DisplayName="Radio Choice">
				<BoundToBoolean SourceControl="CheckBox4" />
				<Choice>Choice 1</Choice>
				<Choice>Choice 2</Choice>
				<Choice>Choice 3</Choice>
			</RadioButton>
		</Controls>
	</Panel>
</IndirectUI>